<flex-config>
	<benchmark>false</benchmark>
	<default-background-color>#FFFFFF</default-background-color>
	<target-player>10</target-player>
	<output>../deploy/geleca.3.0.swc</output>
	
	<compiler>
		<incremental>true</incremental>
		<optimize>true</optimize>
		<keep-generated-actionscript>false</keep-generated-actionscript>
		<warn-no-constructor>true</warn-no-constructor>
		<warn-no-explicit-super-call-in-constructor>true</warn-no-explicit-super-call-in-constructor>
		<show-actionscript-warnings>true</show-actionscript-warnings>
		<show-binding-warnings>true</show-binding-warnings>
		<show-unused-type-selector-warnings>true</show-unused-type-selector-warnings>
		<debug>true</debug>
		


		<!-- Example library linkage -->
		<library-path append="true">
			<path-element>../lib/bin/</path-element>
		</library-path>
		
		<!-- for external swc's
		<external-library-path append="true">
	      <path-element>../lib/skin/</path-element>
	    </external-library-path>
		-->
		

		
		<source-path>
			<path-element>../lib/src/</path-element>
			<!--<path-element>../test/src/</path-element>-->
		</source-path>
		
	</compiler>
	
	<static-link-runtime-shared-libraries>true</static-link-runtime-shared-libraries>
	
	<metadata>
		<title>geleca-framework Application</title>
		<description>geleca-framework Project</description>
		<publisher>Made in TextMate</publisher>
		<creator>Cristiano Caldas</creator>
		<language>EN</language>
	</metadata>
	<raw-metadata></raw-metadata>
</flex-config>
